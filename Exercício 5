>>> a=10
>>> b=4
>>> c=8
>>> print((not a == b) and c < 10)
True
>>> print((a > b) or (4==b))
True
>>> print((a < b) and (not(a==10)))
False
>>> print(((b<a) and (c > b)) or (c==7))
True
